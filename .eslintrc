{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "jasmine": true
  },
  "plugins": [
    "import",
    "react",
    "jasmine",
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": 0,
    "prefer-arrow-callback": 2,
    "react/display-name": 0,
    "semi": [ 2, "never" ]
  }
}